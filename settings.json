{
  "editor.tabCompletion": "onlySnippets",
  "editor.snippetSuggestions": "top",
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#8427e8ef",
    "scrollbarSlider.activeBackground": "#8c3ce2ca"
  },
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.folders.color": "#a3ffbf",
  "editor.fontFamily": "\"SF Mono\"",
  "editor.fontLigatures": true,
  "workbench.editor.wrapTabs": true,
  "editor.wordWrap": "on",

  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "never"
  },
  "indentRainbow.colors": [
    "rgb(153, 221, 204, 0.025);",
    "rgb(255, 93, 162,0.025);",
    "rgb(156, 25, 224,0.025);)",
    "rgb(0, 13, 107,0.025);"
  ],
  "editor.tokenColorCustomizations": {},
  "editor.lineHeight": 2,
  "liveServer.settings.donotShowInfoMsg": true,
  "workbench.colorTheme": "Tailwind - Darkest",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.inlineSuggest.enabled": true,
  "editor.formatOnSave": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "javascript.updateImportsOnFileMove.enabled": "always",

  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "editor.suggestSelection": "first",
  "editor.wordWrapColumn": 100,
  "powermode.combo.timeout": 0,
  "powermode.shake.enabled": false,
  "powermode.explosions.size": 5,
  "powermode.combo.counterEnabled": "hide",
  "powermode.enabled": true,
  "powermode.combo.counterSize": 2,
  "color-highlight.markerType": "underline",
  "git.autofetch": true,
  "git.confirmSync": false,
  "editor.accessibilitySupport": "off",
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on",
    "other": "on"
  },
  "tabnine.experimentalAutoImports": true,
  "editor.fontSize": 15,
  "vscode_custom_css.imports": ["file:///C:/Users/volon/colorsText.css"],
  "security.workspace.trust.untrustedFiles": "open",
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.openRepositoryInParentFolders": "never",
  "editor.mouseWheelZoom": true,
  "terminal.integrated.defaultProfile.windows": "Git Bash"
}
